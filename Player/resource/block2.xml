<BlockGame>
<Setting>
<Screen>
<Size w="1024" h="768"/>
</Screen>
<Bg>
<BgImg bgImg="images/sky.jpg"/>
<BgMusic bgMusic="sounds/bird.wav"/>
<FireSound fireSound="sounds/gun.wav"/>
</Bg>
<GameMode>
<BlockMode blockMode="limit"/>
<ShotMode shotMode="manual"/>
<BlockDirection blockDirection="down"/>
<BulletBlockDirection bulletBlockDirection="cross"/>
</GameMode>
</Setting>
<GamePanel>
<Player>
<BulletBlock blockw="50" blockh="50" blockx="500" blocky="600" bulletw="30" bulleth="50" firetimems="30" bulletdistance="30" blockimg="images/jet.png" bulletimg="images/bullet.png"/>
</Player>
<Stage>
<PlusSpeed plusSpeed="10"/>
<StageBlock stageblockx="500" stageblocky="100" stageblockw="300" stageblockh="30" stagecontent="Stage : " stagefontname="고딕체" stagefontstyle="1" stagefontsize="30" stagefontcolor="10"/>
</Stage>
<Score>
<Rule rule="10"/>
<ScoreBlock scoreblockx="10" scoreblocky="500" scoreblockw="200" scoreblockh="300" scorecontent="Score : " scorefontname="고딕체" scorefontstyle="1" scorefontsize="30" scorefontcolor="10"/>
</Score>
<Block>
<Obj x="300" y="300" w="100" h="100" randscope="0" life="1" speed="1" img="images/sparrow1.png" shotimg="images/shot.png"/>
<Obj x="70" y="100" w="50" h="30" randscope="0" life="2" speed="2" img="images/sparrow1.png" shotimg="images/shot.png"/>
<Obj x="130" y="200" w="80" h="80" randscope="0" life="3" speed="1" img="images/sparrow1.png" shotimg="images/shot.png"/>
<Obj x="230" y="400" w="50" h="30" randscope="0" life="4" speed="1" img="images/minibird1.png" shotimg="images/shot.png"/>
<Obj x="600" y="300" w="50" h="30" randscope="0" life="5" speed="1" img="images/minibird1.png" shotimg="images/shot.png"/>
<Obj x="842" y="543" w="200" h="200" randscope="0" life="6" speed="1" img="images/minibird1.png" shotimg="images/shot.png"/>
</Block>
</GamePanel>
</BlockGame>